{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/storybook/**",
        "destination": "/storybook/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
